             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestWeb"
             mc:Ignorable="d" Height="149.737" Width="256.401">
    <Grid>
        <Canvas HorizontalAlignment="Left" Height="144" VerticalAlignment="Top" Width="253">
            <Label x:Name="label" Content="Shipping method" Canvas.Left="10" Canvas.Top="53"/>
            <Label x:Name="label1" Content="Shipping price" Canvas.Left="26" Canvas.Top="84"/>
            <ComboBox x:Name="comboBox" Canvas.Left="118" Canvas.Top="53" Width="120" SelectionChanged="comboBox_SelectionChanged"/>
            <TextBox x:Name="textBox" Height="23" Canvas.Left="118" TextWrapping="Wrap" Canvas.Top="88" Width="120" IsEnabled="False"/>
            <Label x:Name="label2" Content="Choose sipping method:" Canvas.Left="10" Canvas.Top="15"/>
