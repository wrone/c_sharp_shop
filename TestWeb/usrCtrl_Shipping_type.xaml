<UserControl x:Class="TestWeb.usrCtrl_Shipping_type"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestWeb"
             mc:Ignorable="d" Height="117.855" Width="256.401">
    <Grid Margin="0,0,0,10">
        <Canvas HorizontalAlignment="Left" Height="98" VerticalAlignment="Top" Width="253">
            <Label x:Name="label" Content="Shipping method" Canvas.Left="10" Canvas.Top="10"/>
            <Label x:Name="label1" Content="Shipping price" Canvas.Left="26" Canvas.Top="41"/>
            <ComboBox x:Name="comboBox" Canvas.Left="118" Canvas.Top="10" Width="120" SelectionChanged="comboBox_SelectionChanged"/>
            <TextBox x:Name="textBox" Height="23" Canvas.Left="118" TextWrapping="Wrap" Text="TextBox" Canvas.Top="45" Width="120" IsEnabled="False"/>
            <Button x:Name="button" Content="OK" Canvas.Left="177" Canvas.Top="78" Width="76" Click="button_Click"/>
        </Canvas>

    </Grid>
</UserControl>
