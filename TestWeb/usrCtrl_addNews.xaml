<UserControl x:Class="TestWeb.usrCtrl_addNews"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestWeb"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="804" Height="414">
    <Grid>
        <Label x:Name="label" Content="News name:" HorizontalAlignment="Left" Margin="79,77,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="name" HorizontalAlignment="Left" Height="23" Margin="158,79,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="323"/>
        <Label x:Name="label1" Content="News text:" HorizontalAlignment="Left" Margin="88,108,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="text" HorizontalAlignment="Left" Height="160" Margin="158,108,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="323"/>
        <Button x:Name="addEditButton" Content="Add" HorizontalAlignment="Left" Margin="406,310,0,0" VerticalAlignment="Top" Width="75" Click="addEditButton_Click"/>
    </Grid>
</UserControl>
